{
  "name": "@zartin/react-ng",
  "version": "0.0.4",
  "scripts": {
    "build-builder": "rimraf ./dist/react-ng && tsc -p projects/react-ng/tsconfig.json && npm run copy-pkg && npm run copy-static-files",
    "copy-pkg": "copyfiles -u 2 projects/react-ng/package.json dist/react-ng",
    "copy-static-files": "copyfiles -u 3 'projects/react-ng/src/**/*.json' 'projects/react-ng/src/**/files/**/*' projects/react-ng/src/builders/config/* dist/react-ng"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/architect": "~0.1100.0",
    "@angular-devkit/build-angular": "~0.1100.0",
