    "rimraf": "^3.0.2",
    "rxjs": "~6.6.0",
    "sass-loader": "^10.1.0",
    "source-map-loader": "^2.0.0",
    "style-loader": "^0.23.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.0.0",
    "typescript": "^4.0.0",
    "web-vitals": "^0.2.4",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.0.0"
  },
