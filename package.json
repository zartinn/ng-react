    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/node": "^12.11.1",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.0.2",
    "babel-preset-react-app": "^10.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^1.0.0",
    "file-loader": "^6.2.0",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
